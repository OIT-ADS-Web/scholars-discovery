PREFIX vitro: <http://vitro.mannlib.cornell.edu/ns/vitro/0.7#>
PREFIX vivo: <http://vivoweb.org/ontology/core#>

CONSTRUCT {
    ?document a <[(${uri})]> .
    ?document ?property ?value .

} WHERE {
    ?document a <[(${uri})]> .
    ?document ?property ?value .
    
    FILTER (?property != vitro:mostSpecificType || (?property = vitro:mostSpecificType && ?value != vivo:DateTimeValue))
}