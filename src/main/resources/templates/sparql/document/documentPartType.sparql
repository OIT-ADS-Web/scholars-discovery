PREFIX obo: <http://purl.obolibrary.org/obo/>
PREFIX vitro: <http://vitro.mannlib.cornell.edu/ns/vitro/0.7#>
PREFIX vivo: <http://vivoweb.org/ontology/core#>

CONSTRUCT {
    ?documentPart vitro:mostSpecificType ?typeWithId .
} WHERE {
    <[(${uri})]> obo:BFO_0000051 ?documentPart .
    ?documentPart vitro:mostSpecificType ?type .
	BIND( CONCAT( STR(?type), REPLACE(STR(?documentPart), "(^.*/)", "::") ) AS ?typeWithId ) .
    FILTER (?type != vivo:DateTimeValue) .
}