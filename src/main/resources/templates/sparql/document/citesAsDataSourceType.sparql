PREFIX cito: <http://purl.org/spar/cito/>
PREFIX vitro: <http://vitro.mannlib.cornell.edu/ns/vitro/0.7#>

CONSTRUCT {
    ?dataSource vitro:mostSpecificType ?mostSpecificType .
} WHERE {
    <[(${uri})]> cito:citesAsDataSource ?dataSource .
    ?dataSource vitro:mostSpecificType ?mostSpecificType .
}