PREFIX vivo: <http://vivoweb.org/ontology/core#>
PREFIX vitro: <http://vitro.mannlib.cornell.edu/ns/vitro/0.7#>

CONSTRUCT {
    ?publisher vitro:mostSpecificType ?typeWithId .
} WHERE {
    <[(${uri})]> vivo:publisher ?publisher .
    ?publisher vitro:mostSpecificType ?type .
    BIND( CONCAT( STR(?type), REPLACE(STR(?publisher), "(^.*/)", "::") ) AS ?typeWithId ) .
}